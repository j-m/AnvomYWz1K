  "main": "./src/main.js",
  "engines": {
    "node": "12.0.0",
    "npm": "6.12.0"
  },
  "scripts": {
    "coverage": "jest --coverage --testPathIgnorePatterns='./test/acceptance/puppeteer/*'",
    "debug": "node --inspect ./src/main.js",
    "linter": "eslint . --ignore-pattern /static/*",
    "puppeteer": "jest ./test/acceptance/puppeteer --runInBand --bail --u",
    "start": "node ./src/main.js",
    "test": "jest --testPathIgnorePatterns='./test/acceptance/puppeteer/*'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.coventry.ac.uk/340CT-1920SEPJAN/marshj6.git"
  },
  "keywords": [
    "game",
    "review",
    "website",
    "coursework"
  ],
  "author": "Jonathan Marsh <marshj6@coventry.ac.uk>",
  "license": "UNLICENSED",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcrypt-promise": "^2.0.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.7",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-encrypted-session": "^3.0.7",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "koa-views": "^7.0.0",
    "sqlite-async": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.3.0",
    "jest-puppeteer": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^5.5.0",
    "puppeteer-har": "^1.1.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/static/js",
      "<rootDir>/node_modules"
    ]
  }
}
